@model List<MarvinBlogv._2._0.Models.Post>
@{
    Layout = "_AdminDashboardLayout";
}


<div class="container">
    <div class="row">

        @foreach (var post in Model)
        {
            <div class="col-4 mb-4 col-lg-4">
                <div class="card">
                    <div class="card-header ">
                        <h5 class="card-title text-center" style="color:blue">@post.Title</h5>

                        <div class="card-body">
                            <div class="card" style="color:hotpink"></div>
                            Created By: @post.CreatedBy<br /><br />
                            Post description: @post.Description<br /><br />
                            Post Categories: @post.CreatedAt<br /><br />
                        </div>
                    </div>
                </div>
            </div>
        }
    </div>
</div>

